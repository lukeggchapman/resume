  "author": "Luke Chapman <lukeggchapman@gmail.com>",
  "engines": {
    "yarn": "^1.2.1",
    "node": ">11.0.0"
  },
  "dependencies": {
    "@babel/core": "^7.4.3",
    "@react-pdf/renderer": "^1.5.4",
    "date-fns": "^1.30.1",
    "mdast-util-to-hast": "^6.0.1",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-uid": "^2.2.0",
    "remark": "^10.0.1",
    "typescript": "^3.4.1"
  },
  "devDependencies": {
    "@babel/node": "^7.4.5",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.3.3",
    "@types/enzyme": "^3.9.1",
    "@types/enzyme-adapter-react-16": "^1.0.5",
    "@types/hast-format": "^2.3.0",
    "@types/html-webpack-plugin": "^3.2.0",
    "@types/jest": "^24.0.11",
    "@types/mdast": "^3.0.2",
    "@types/node": "^12.0.8",
    "@types/react-dom": "^16.8.4",
    "@types/webpack": "^4.4.32",
    "@types/webpack-dev-server": "^3.1.6",
    "@types/webpack-merge": "^4.1.5",
    "@typescript-eslint/eslint-plugin": "^1.6.0",
    "@typescript-eslint/parser": "^1.6.0",
    "babel-jest": "^24.8.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-module-resolver": "^3.2.0",
    "codeclimate-test-reporter": "^0.5.1",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.12.1",
    "enzyme-to-json": "^3.3.5",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-import-resolver-webpack": "^0.11.1",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-jest": "^22.4.1",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.4",
    "eslint-plugin-react-hooks": "^1.6.0",
    "file-loader": "^4.0.0",
    "gray-matter-loader": "^0.0.10",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^2.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.7.1",
    "jest-enzyme": "^7.0.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.16.4",
    "react-hot-loader": "^4.11.0",
    "react-test-renderer": "^16.8.6",
    "ts-node": "^8.2.0",
    "url-loader": "^2.0.0",
    "webpack": "^4.34.0",
    "webpack-cli": "^3.3.4",
    "webpack-dev-server": "^3.7.1",
    "webpack-merge": "^4.2.1"
  },
  "license": "MIT",
  "scripts": {
    "prebuild": "rm -rf dist",
    "build": "npm-run-all -p build:types build:js -s build:pdf",
    "build:types": "tsc --emitDeclarationOnly --project tsconfig.prd.json",
    "build:js": "webpack --config webpack.prd.ts",
    "build:pdf": "NODE_ENV=production ts-node scripts/build.ts dist/LukeChapman-Resume.pdf",
    "develop": "webpack-dev-server --open --config webpack.dev.ts",
    "dev": "yarn develop",
    "format": "prettier --write src/**/*.{js,jsx}",
    "lint": "eslint \"src/**/*.{ts,tsx}\"",
    "lint:fix": "yarn lint --fix",
    "test": "jest --verbose",
    "test:watch": "yarn test --watch",
    "coverage": "yarn test --ci --coverage",
    "type-check": "tsc --noEmit --project tsconfig.prd.json",
    "ci": "run-p lint type-check coverage"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lukeggchapman/resume"
  },
  "bugs": {
    "url": "https://github.com/lukeggchapman/resume/issues"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn ci"
    }
  }
}
